{
  "name": "@effectai/effect-js",
  "version": "0.3.47",
  "description": "Effect Network Javscript/Typescript SDK (for [https://effect.network](https://effect.network))",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest --coverage",
    "test:unit": "vitest --config=test/unit/vitest.unit.config.js",
    "test:e2e": "vitest --config=test/e2e/vitest.e2e.config.js",
    "test:integration": "vitest --config=test/e2e/vitest.integration.config.js",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "build": "npm run build:esm",
    "build:cjs": "tsc --module commonjs",
    "build:esm": "tsc --module es2020",
    "watch": "npm run prebuild && tsc --watch",
    "prebuild": "rimraf docs dist",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch --emit both",
    "lint": "eslint \"src/**\"",
    "prepare": "husky install",
    "publish:public": "npm run prebuild && npm version patch && npm run build:cjs && npm publish --access public",
    "publish:next": "npm run prebuild && npm version patch && npm run build && npm publish --tag next",
    "publish:cjs": "npm run prebuild && npm version patch && npm run build:cjs && npm publish --tag cjs",
    "publish:esm": "npm run prebuild && npm version patch && npm run build:esm && npm publish --tag esm",
    "publish:rc": "npm run prebuild && npm version patch && npm run build:esm && npm publish --tag rc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/effectai/effect-js.git"
  },
  "keywords": [
    "effect",
    "efx",
    "future",
    "work",
    "ai",
    "crypto",
    "blockchain",
    "microtask",
    "task",
    "micro-task"
  ],
  "author": {
    "name": "Effect-AI",
    "url": "https://effect.network",
    "email": "dev@effect.ai"
  },
  "contributors": [
    {
      "name": "Laurens Verspeek",
      "email": "laurens@effect.ai"
    },
    {
      "name": "David Britt",
      "email": "david@effect.ai"
    },
    {
      "name": "Ja√Ør Zijp",
      "email": "jair@effect.ai"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/effectai/effect-js/issues"
  },
  "homepage": "https://github.com/effectai/effect-js#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.5.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-v8": "^0.34.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "stream-http": "^3.2.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-mermaid": "^1.10.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@wharfkit/antelope": "^0.8.0",
    "@wharfkit/session": "^1.0.0",
    "@wharfkit/wallet-plugin-privatekey": "^1.0.0"
  }
}
